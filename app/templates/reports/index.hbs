<h1>Reports</h1>
{{link-to 'Create report' 'reports.new' }}
<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Submitter</th>
      <th>isValid</th>
    </tr>
  </thead>
  <tbody>

  {{#each model as |report|}}

    <tr>
      <td>{{link-to report.id 'reports.show' report }}</td>
      <td>{{ report.submitterId }}</td>
      <td>{{ report.valid }}</td>
    </tr>

  {{/each}}

  </tbody>
</table>